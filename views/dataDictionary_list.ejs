<script id="itemTpl" type="text/html">
<div class="row">
	<input type="hidden" name="dataInfoId">
	<input type="text" name="code" class="col-sm-2 input-sm" placeholder="字典编码" title="字典编码">
	<input type="text" name="label_zh" class="col-sm-2 input-sm" placeholder="名称(中文)" title="字典名称(中文)">
	<input type="text" name="label_en" class="col-sm-2 input-sm" placeholder="名称(英文)" title="字典名称(英文)">
	<input type="text" name="innerValue" class="col-sm-2 input-sm" placeholder="内部值" title="内部使用数值">
	<div class="col-xs-12 col-sm-4">
		<div class="btn-group btn-group-xs pull-right">
			<button data-comm="onoff" type="button" class="btn btn-default" title="禁用/启用"><i class="glyphicon glyphicon-ban-circle"></i></button>
			<button data-comm="goup" type="button" class="btn btn-default" title="上移"><i class="glyphicon glyphicon-arrow-up"></i></button>
			<button data-comm="godown" type="button" class="btn btn-default" title="下移"><i class="glyphicon glyphicon-arrow-down"></i></button>
			<button data-comm="del" type="button" class="btn btn-default" title="删除"><i class="glyphicon glyphicon-remove"></i></button>
		</div>
	</div>
</div>
</script>

<script id="addEditTpl" type="text/html">
