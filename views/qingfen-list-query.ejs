<script id="row_template" type="text/html">
<$
	var item;
	for(var i=0;i<data.length;i++){
		item = data[i];
$>
	<tr role="row" id="1" tabindex="-1" class="ui-widget-content jqgrow ui-row-ltr" aria-selected="false">
		<td role="gridcell" style="text-align:center;width: 25px;" aria-describedby="grid_cb"><input role="checkbox" type="checkbox" id="jqg_grid_1" class="cbox" name="jqg_grid_1"></td>
